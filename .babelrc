{
  "presets": [
    ["@babel/preset-env", { 
      "targets": { "node": "current" },
      "modules": "commonjs"
    }],
    ["@babel/preset-react", {
      "runtime": "automatic"
    }],
    ["@babel/preset-typescript", { 
      "isTSX": true, 
      "allExtensions": true 
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "regenerator": true,
      "helpers": true
    }]
  ]
}
